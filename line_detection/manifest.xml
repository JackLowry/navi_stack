<package>
	<description brief="Detection of painted road lines">
		Detects painted road lines in an image. Assumes a fully calibrated
		camera and a priori knowledge of line width.
	</description>
	<author>Michael Koval/koval.michael@gmail.com</author>
	<license>BSD</license>
	<review status="unreviewed" notes=""/>
	<export>
		<nodelet plugin="${prefix}/nodelet.xml"/>
	</export>
	<depend package="cv_bridge"/>
	<depend package="image_geometry"/>
	<depend package="image_transport"/>
	<depend package="opencv2"/>
	<depend package="nodelet"/>
	<depend package="pcl"/>
	<depend package="pcl_ros"/>
	<depend package="rosconsole"/>
	<depend package="sensor_msgs"/>
	<depend package="tf"/>
	<depend package="visualization_msgs"/>
</package>
